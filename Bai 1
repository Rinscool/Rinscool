#include <stdio.h>
int main() {
    int a, b, sum = 0;
    printf("nhap a, b:");
    scanf("%d %d", &a ,&b );
    if ( a < b){
        printf("vui long nhap lai.\n");
        return 1;
    }
    int i = a;
    do{
        sum += i++;
    }while (i <= b);
    printf("Tong tu %d den %d la: %d\n", a, b, sum);
    return 0;
}
